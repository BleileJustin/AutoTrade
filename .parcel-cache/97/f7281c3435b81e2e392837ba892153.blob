* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

@font-face {
  font-family: Eva;
  src: url(a5cbe1f0beafd0f4660042b62ce17cb8);
}

body {
  display: flex;
  flex-direction: column;
}

h1,
h2 {
  font-family: Eva;
  color: white;
}

.title {
  margin: 0.5em;
  margin-left: 0.7em;
}

.top_bar {
  width: 100%;
  height: 8vh;
  background-color: #2c8c99;
  display: flex;
  align-items: center;
}

.dashboard-container-bottom,
.dashboard-container-top {
  display: flex;
  justify-content: space-around;
  margin-top: 2vh;
}

.analytics {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0.5em;
  width: 25%;
  height: 50vh;
  background-color: #326771;
}

.profit_container {
  margin-top: 0.5em;
  padding: 0.5em;
  width: 90%;
  height: 10vh;
  background-color: #2c8c99;
}

.strategy_contatiner {
  margin-top: 0.5em;
  padding: 0.5em;
  width: 90%;
  height: 30vh;
  background-color: #2c8c99;
}

.graph {
  padding: 0.5em;
  width: 73%;
  height: 50vh;
  background-color: #326771;
}

.open,
.filled {
  display: flex;
  justify-content: center;
  padding: 0.5em;
  width: 48%;
  height: 35vh;
  background-color: #326771;
}
