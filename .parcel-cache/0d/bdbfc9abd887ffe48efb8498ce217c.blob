* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

@font-face {
  font-family: Eva;
  src: url(Eva-Light.HASH_REF_4a6f7b23be409ee1d595c4440a4aede3.ttf);
}

body {
  display: flex;
  flex-direction: column;
}

h1,
h2 {
  font-family: Eva;
  color: white;
}

.title {
  margin: 0.5em;
  margin-left: 0.7em;
}

.top_bar {
  width: 100%;
  height: 8vh;
  background-color: #2c8c99;
  display: flex;
  align-items: center;
}

.dashboard-container-bottom,
.dashboard-container-top {
  display: flex;
  justify-content: space-around;
  margin-top: 2vh;
}

.dashboard-container-bottom:before,
.dashboard-container-top:before,
.dashboard-container-bottom:after,
.dashboard-container-top:after {
  content: "";
}

.analytics {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0.5em;
  width: 23%;
  height: 50vh;
  background-color: #326771;
}

.profit_container {
  margin-top: 0.5em;
  padding: 0.5em;
  width: 90%;
  height: 10vh;
  background-color: #2c8c99;
}

.strategy_contatiner {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-top: 0.5em;
  padding: 0.5em;
  width: 90%;
  height: 30vh;
  background-color: #2c8c99;
}

#start {
  color: white;
  background-color: #326771;
  border-width: 2;
  border-color: white;
  height: 3vh;
}

.graph {
  padding: 0.5em;
  width: 72%;
  height: 50vh;
  background-color: #326771;
}

.open,
.filled {
  display: flex;
  justify-content: center;
  padding: 0.5em;
  width: 47.5%;
  height: 35vh;
  background-color: #326771;
}

