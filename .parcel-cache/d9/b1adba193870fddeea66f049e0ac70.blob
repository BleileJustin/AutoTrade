* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

@font-face {
  font-family: Eva;
  src: url(7d1912e437c40619066aa4edfe6a8434);
}

body {
  display: flex;
  flex-direction: column;
}

h2 {
  font-family: Eva;
  color: white;
}

.top_bar {
  width: 100%;
  height: 10vh;
  background-color: #2c8c99;
}

.dashboard-container-top {
  display: flex;
  justify-content: space-around;
}

.dashboard-container-bottom {
  display: flex;
  justify-content: space-around;
}

.analytics {
  width: 20%;
  height: 50vh;
  background-color: #326771;
}

.graph {
  width: 70%;
  height: 50vh;
  background-color: #326771;
}

.open,
.filled {
  width: 45%;
  height: 40vh;
  background-color: #326771;
}
