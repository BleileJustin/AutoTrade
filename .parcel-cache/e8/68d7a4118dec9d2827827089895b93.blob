* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

@font-face {
  font-family: Eva;
  src: url(Eva-Light.HASH_REF_4b21da97d3989c6e735eb955acfad91a.ttf);
}

@font-face {
  font-family: Simplifica;
  src: url(Simplifica-Regular.HASH_REF_b6414a6b7dc50ae1aa5de08b0e0083db.ttf);
}

body {
  display: flex;
  flex-direction: column;
  background-image: url(topoNEU.HASH_REF_ea4e5b324d58f163446af7541f1d6cc7.jpg);
  background-size: 100% 100%;
}

h1 {
  font-family: Eva;
  color: white;
}

h2,
h3,
h4 {
  font-family: Simplifica;
  letter-spacing: 0.1em;
  line-height: 1.2em;
  color: white;
  padding: 0.1em;
}

.title {
  margin: 0.5em;
  margin-left: 0.7em;
}

.top_bar {
  width: 100%;
  height: 8vh;
  background-color: #2c8c99;
  display: flex;
  align-items: center;
}

.dashboard-container-bottom,
.dashboard-container-top {
  display: flex;
  justify-content: space-around;
  margin-top: 2vh;
}

.dashboard-container-bottom:before,
.dashboard-container-top:before,
.dashboard-container-bottom:after,
.dashboard-container-top:after {
  content: "";
}

.analytics {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0.5em;
  width: 100%;
  height: 50vh;
  background-color: #326771;
}

.position {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  margin-top: 0.5em;
  padding: 0.5em 0.5em 0.5em 0.5em;
  width: 95%;
  height: 12vh;
  background-color: #2c8c99;
}

.portfolio_value,
.position {
  border: 1px black;
}

#currency-dropdown,
#exchange-dropdown,
#strategy-dropdown {
  width: 90%;
  height: 1.8em;
  border: 0;
  background-color: #326771;
  color: white;
  text-align: center;
  font-family: Simplifica;
  letter-spacing: 0.2em;
  padding: 0.3em;
}

.strategy_contatiner {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-top: 0.5em;
  padding: 0.5em;
  width: 90%;
  height: 5vh;
  background-color: #2c8c99;
}

.button-container {
  display: flex;
  justify-content: space-around;
  font-size: 0.75em;
}

#start,
#stop {
  padding: 0.2em;
  color: black;
  border-width: 0;
  height: 3vh;
}

#start {
  background-color: #06d6a0;
  font-family: Simplifica;
  letter-spacing: 0.1em;
  padding: 0.3em;
  width: 48%;
  font-size: 0.9em;
  text-align: center;
}

#stop {
  background-color: #f94144;
  color: white;
  font-family: Simplifica;
  letter-spacing: 0.1em;
  padding: 0.3em;
  width: 48%;
  font-size: 0.9em;
  text-align: center;
}

.parameter {
  display: flex;
  flex-direction: row;
}

