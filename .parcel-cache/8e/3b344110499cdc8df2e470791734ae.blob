* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

@font-face {
  font-family: Eva;
  src: url(a5cbe1f0beafd0f4660042b62ce17cb8);
}

body {
  display: flex;
  flex-direction: column;
}

h1,
h2 {
  font-family: Eva;
  color: white;
}

.title {
  margin: 0.5em;
}

.top_bar {
  width: 100%;
  height: 5vh;
  background-color: #2c8c99;
  display: flex;
  align-items: center;
}

.dashboard-container-top {
  display: flex;
  justify-content: space-around;
}

.dashboard-container-bottom {
  display: flex;
  justify-content: space-around;
}

.analytics {
  width: 20%;
  height: 50vh;
  background-color: #326771;
}

.graph {
  width: 70%;
  height: 50vh;
  background-color: #326771;
}

.open,
.filled {
  width: 45%;
  height: 40vh;
  background-color: #326771;
}
